{
  "type": "minecraft:multi_action",
  "title": "Merge - Clumps Config",
  "can_close_with_escape": true,
  "pause": true,
  "exit_action": {
    "label": "Back",
    "tooltip": "Click to go back",
    "action": {
      "type": "run_command",
      "command": "/dialog show @s clumps:config"
    }
  },
  "columns": 1,
  "actions": [
    {
      "label": "Enable",
      "tooltip": "Click to enable merge",
      "width": 200,
      "action": {
        "type": "run_command",
        "command": "/scoreboard players set merge clumps.config 1"
      }
    },
        {
      "label": "Disable",
      "tooltip": "Click to disable merge",
      "width": 200,
      "action": {
        "type": "run_command",
        "command": "/scoreboard players set merge clumps.config 0"
      }
    }
  ]
}